<p id="notice"><%= notice %></p>

<h1>Products</h1>
<%= form_tag url: search_path, remote: true %>
  <%= text_field_tag :search_str, '' %>
  <%= submit_tag 'Send' %>
<% end %>
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Price</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <%= render partial: 'list' %>
    </tbody>
  </table>
<%= will_paginate @posts %>


<br>

<%= link_to 'New Product', new_product_path %>
